<%- include('../partials/header') %>
        <!-- Container Fluid-->
        <div class="container-fluid" id="container-wrapper">
          <div class="d-sm-flex align-items-center justify-content-between mb-4">
            <h1 class="h3 mb-0 text-gray-800">Grades</h1>
          </div>

          <!-- Row -->
          <div class="row">
            <!-- Grades -->
            <div class="col-lg-12">
              <div class="card mb-4">
                <div class="card-header py-3 d-flex flex-row align-items-center justify-content-between">
                  <h6 class="m-0 font-weight-bold text-primary">Grades</h6>
                  <a class="m-0 float-right btn btn-danger btn-sm" href="/admin/grade/search">Add Grade <i class="fas fa-chevron-right"></i></a>
                </div>

                <div class="table-responsive p-3">
                  <table class="table align-items-center table-flush table-hover" id="dataTableHover">
                    <thead class="thead-light">
                      <tr>
                        <th>No</th>
                        <th>Student ID</th>
                        <th>Course</th>
                        <th>Grade</th>
                        <!-- <th>Academic Year</th> -->
                        <th>Student Year</th>
                        <th colspan="2" class="text-center">Action</th>
                      </tr>
                    </thead>
                    <tfoot>
                      <tr>
                        <th>No</th>
                        <th>Student ID</th>
                        <th>Course</th>
                        <th>Grade</th>
                        <!-- <th>Academic Year</th> -->
                        <th>Student Year</th>
                        <th colspan="2" class="text-center">Action</th>
                      </tr>
                    </tfoot>
                    <tbody>
                      <% let count = 1; %>
                      <% grades.forEach((grade) => { %>
                        <tr>
                          <td><%- count %></td>
                          <td><%- grade.studentID %></td>
                          <td><%- grade.course %></td>
                          <td><%- grade.grade %></td>
                          <!-- <td><%- //grade.academicYear %></td> -->
                          <td><%- grade.student_year %></td>
                          <td><a href="/admin/grade/<%- grade._id %>/edit" class="btn btn-sm btn-primary">Edit</a></td>
                          <td>
                            <form action="/admin/grade/<%-grade._id %>?_method=DELETE" method="POST">
                              <button type="submit" class="btn btn-sm btn-danger">Delete</button>
                            </form>
                          </td>
                        </tr>
                        <% count++; %>
                      <% }); %>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <!--Row-->
          
<%- include("../partials/footer") %>